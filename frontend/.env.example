# GeoSafe Frontend Environment Configuration
# Copy this to .env.local and adjust values as needed

# Backend API URL
REACT_APP_API_URL=http://localhost:8000

# Map tile server (optional, defaults to OpenStreetMap)
REACT_APP_TILE_SERVER_URL=https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png

# Default map center (latitude, longitude)
REACT_APP_MAP_CENTER_LAT=41.0082
REACT_APP_MAP_CENTER_LNG=28.9784
REACT_APP_MAP_DEFAULT_ZOOM=12
